@model Funcionario

@{
    ViewData["Title"] = "Deletar Funcionário";
}

<div class="text-center">
    <h3>Realmente deseja apagar o cliente (@Model.Nome)?</h3>
</div>

<a class="btn btn-secondary btn-block w-100 py-2" role="button" asp-controller="Funcionario" asp-action="Index">Cancelar exclusão</a>
<a class="btn btn-danger btn-block w-100 py-2" role="button" asp-controller="Funcionario" asp-action="Delete" asp-route-id="@Model.Id">Confirmar exclusão</a>